//定义一个含有5个元素的数组，实现数组接受键盘输入的数字，输入完毕后打印数组元素，
//比较得出最大值和最小值，并输出。
#include<stdio.h>
main(){
    int max,min,i,t[5];
for(i=0;i<5;i++)
    scanf(%d,&t[i]);
max=t[0];
min=t[0];
for(i=1;i<5;i++){
    if(max<t[i])    max=t[i];
if(min>t[i])     min=t[i];
}
for(i=0;i<5;i++)
